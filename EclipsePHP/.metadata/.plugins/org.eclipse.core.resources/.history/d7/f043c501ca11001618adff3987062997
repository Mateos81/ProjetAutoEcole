 /**
  * Classe d'accs aux donnes.
  * DAL pour Data Access Layer.
  */
class TypeL {
    
    /** Mise en place d'un singleton */
    private static $instance;
    
    /** Tableau avec pour cl le numro et pour valeur le type d'examen */
    private $typeL;
    
    /** */
    public function __construct() {
        
    }
    
    /**
     * Accesseur sur le tableau des types d'examen.
     */
    public function getTypeL() {
        return $this->typeL;
    }
    
}
php?>